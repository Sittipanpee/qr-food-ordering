# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_RESTAURANT_NAME="My Restaurant"

# Admin Authentication
# Use: node -e "console.log(require('bcrypt').hashSync('your-password', 10))"
ADMIN_PASSWORD_HASH=$2b$10$YourHashedPasswordHere
JWT_SECRET=your-jwt-secret-key-change-in-production

# Mode Configuration (restaurant or market)
NEXT_PUBLIC_DEFAULT_MODE=market

# Queue System
QUEUE_SECRET=queue-secret-key-change-in-production

# Image Upload (Optional - Cloudinary)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
